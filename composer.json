{
    "name": "ba66e77/numerish",
    "type": "library",
    "description": "A PHP-based homage to the dying Numerous app.",
    "keywords": [
        "ba66e77",
        "numerish"
    ],
    "homepage": "https://github.com/ba66e77/numerish",
    "license": "MIT",
    "authors": [
        {
            "name": "Barrett Smith",
            "email": "barrett@datasmith.net",
            "homepage": "https://www.datasmith.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "ba66e77\\numerish\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ba66e77\\numerish\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
